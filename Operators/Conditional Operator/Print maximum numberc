// Conditional Operator

#include<stdio.h>
int main() {
    int a, b, c, max;
    printf("Enter value of a: \n");
    scanf("%d", &a);
    printf("Enter value of b: \n");
    scanf("%d", &b);
    printf("Enter value of c: \n");
    scanf("%d", &c);
    max = (a>b && a>c)?a:(b>c?b:c);
    printf("Maximum number is: %d \n", max);
}